{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
	{
	    "name": "f9 with nucleo-f429zi debug",
	    "cwd": "${workspaceFolder}",
	    "executable": "${workspaceFolder}/build/nucleof429/f9.elf",
	    "request": "launch",
	    "type": "cortex-debug",
	    // openocd config
	    "servertype": "openocd",
	    "searchDir": [
		"/usr/local/share/openocd/tcl"
	    ],
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
	    // end of openocd config
	    "gdbPath": "/usr/bin/gdb-multiarch",
	    "armToolchainPath": "/usr/bin",
	    "toolchainPrefix": "arm-none-eabi",
	    "runToEntryPoint": "main",
	    "device": "STM32F429ZITX",
	    "svdFile": "${workspaceFolder}/svd/STM32F429x.svd",
	    "liveWatch": {
		"enabled": false,
		"samplesPerSecond": 4
	    },
	    "showDevDebugOutput": "none",
	    "preLaunchCommands": [
		"source ${workspaceFolder}/.vscode/f9-gdbinit-adv.gdb"
	    ]
	}
    ]
}
